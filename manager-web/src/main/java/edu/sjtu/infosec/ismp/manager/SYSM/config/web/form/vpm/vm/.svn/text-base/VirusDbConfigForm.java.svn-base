package edu.sjtu.infosec.ismp.manager.SYSM.config.web.form.vpm.vm;


import org.apache.struts.action.ActionForm;

public class VirusDbConfigForm extends ActionForm {
	
	private static final long serialVersionUID = 1L;
	/**
	 * 配置命名
	 * 病毒 瑞星数据库：rav
	 * 补丁 WSUS数据库：wsus
	 */
	private String name1;
	/**
	 * 数据库连接地址
	 */
	private String dbUrl1;
	/**
	 * 数据库驱动
	 */
	private String dbDriver1;
	/**
	 * 数据库访问用户名
	 */
	private String username1;
	/**
	 * 数据库访问密码
	 */
	private String password1;
	
	public String getName1() {
		return name1;
	}
	public void setName1(String name1) {
		this.name1 = name1;
	}
	public String getDbUrl1() {
		return dbUrl1;
	}
	public void setDbUrl1(String dbUrl1) {
		this.dbUrl1 = dbUrl1;
	}
	public String getDbDriver1() {
		return dbDriver1;
	}
	public void setDbDriver1(String dbDriver1) {
		this.dbDriver1 = dbDriver1;
	}
	public String getUsername1() {
		return username1;
	}
	public void setUsername1(String username1) {
		this.username1 = username1;
	}
	public String getPassword1() {
		return password1;
	}
	public void setPassword1(String password1) {
		this.password1 = password1;
	}
}
