package org.infosec.ismp.agent.comm.winsensor.model.status;

import java.io.Serializable;

/**
 * @author Rocky
 * @version create time：Oct 15, 2010 10:59:28 AM
 * 
 */
public class LocalRouter implements Serializable {

	private static final long serialVersionUID = -3027704313721575022L;

	/*
	 * 目的IP
	 */
	private String destination;
	
	/*
	 * 目标掩码
	 */
	private String netMask;
	
	/*
	 * 网关地址
	 */
	private String gateway;
	
	/*
	 * 优先级
	 */
	private String metric;

	public String getDestination() {
		return destination;
	}

	public void setDestination(String destination) {
		this.destination = destination;
	}

	public String getNetMask() {
		return netMask;
	}

	public void setNetMask(String netMask) {
		this.netMask = netMask;
	}

	public String getGateway() {
		return gateway;
	}

	public void setGateway(String gateway) {
		this.gateway = gateway;
	}

	public String getMetric() {
		return metric;
	}

	public void setMetric(String metric) {
		this.metric = metric;
	}
}
