public class CommWindowsUpdateStrategy extends CommBaseStrategy {

	private static final long serialVersionUID = -3128991531008497996L;

	private String url;		//Config file URL
	
	private int interval;		//Update strategy interval.

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public int getInterval() {
		return interval;
	}

	public void setInterval(int interval) {
		this.interval = interval;
	}
}
