package org.infosec.ismp.agent.comm.winsensor.model.strategy;


/**
 * @author Rocky
 * @version create time：Dec 6, 2010 2:09:58 PM
 * 
 */
public class CommWindowsLogStrategy extends CommBaseStrategy {

	private static final long serialVersionUID = 8710532716159004888L;
	
	private String url;		//上报地址
	
	private int interval;		//上报周期
	
	private Boolean system;			//是否采集系统日志
	
	private Boolean application;			//是否采集应用程序日志
	
	private Boolean security;			//是否采集安全日志
	
	private Boolean information;			//是否采集消息
	
	private Boolean warning;			//是否采集警告
	
	private Boolean error;			//是否采集错误
	
	private Boolean aSuccess;			//是否采集成功审核
	
	private Boolean aFailure;			//是否采集失败审核

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public int getInterval() {
		return interval;
	}

	public void setInterval(int interval) {
		this.interval = interval;
	}

	public Boolean getSystem() {
		return system;
	}

	public void setSystem(Boolean system) {
		this.system = system;
	}

	public Boolean getApplication() {
		return application;
	}

	public void setApplication(Boolean application) {
		this.application = application;
	}

	public Boolean getSecurity() {
		return security;
	}

	public void setSecurity(Boolean security) {
		this.security = security;
	}

	public Boolean getInformation() {
		return information;
	}

	public void setInformation(Boolean information) {
		this.information = information;
	}

	public Boolean getWarning() {
		return warning;
	}

	public void setWarning(Boolean warning) {
		this.warning = warning;
	}

	public Boolean getError() {
		return error;
	}

	public void setError(Boolean error) {
		this.error = error;
	}

	public Boolean getASuccess() {
		return aSuccess;
	}

	public void setASuccess(Boolean success) {
		aSuccess = success;
	}

	public Boolean getAFailure() {
		return aFailure;
	}

	public void setAFailure(Boolean failure) {
		aFailure = failure;
	}
}
