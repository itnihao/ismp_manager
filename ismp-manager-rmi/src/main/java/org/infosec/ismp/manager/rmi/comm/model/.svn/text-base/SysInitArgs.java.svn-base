package org.infosec.ismp.manager.rmi.comm.model;

public class SysInitArgs {
	/**
	 * web基本路径
	 */
	public static String WEB_URL_BASE = "";
	/**
	 * 全局属性
	 */
	private static SysInitArgs sysInitArgs = null;
	
	
	
	
	/**
	 * 构造器
	 */
	private SysInitArgs(){}

	
	
	
	/**
	 * 实例化全局属性
	 * @return 全局属性
	 */
	public static SysInitArgs getInstance(){
		if(sysInitArgs == null){
			sysInitArgs = new SysInitArgs();
		}
		return sysInitArgs;
	}
}
