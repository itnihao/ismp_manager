package org.infosec.ismp.manager.rmi.event.modle;

import java.io.Serializable;

/**
 * trap event wrapper
 * 
 * @author lianglin
 * 
 */
public class TrapEventWrapper implements Serializable {
	private TrapEvent event;
	private String nodeId;
	private String ip;

	public TrapEventWrapper(TrapEvent event2) {
		this.event =event2;
	}

	public TrapEvent getEvent() {
		return event;
	}

	public void setEvent(TrapEvent event) {
		this.event = event;
	}


	public String getNodeId() {
		return nodeId;
	}

	public void setNodeId(String nodeId) {
		this.nodeId = nodeId;
	}

	public String getIp() {
		return ip;
	}

	public void setIp(String ip) {
		this.ip = ip;
	}

}
