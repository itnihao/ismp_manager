package org.infosec.ismp.manager.rmi.event.dao.queryCondition;

import org.infosec.ismp.manager.rmi.event.util.Page;

/**
 * Eventmoni的查询条件
 * @author wudengke 2009-6-29
 *
 */
public class EventmoniCondition {
	
	//根据设备名称查询
	private String faci_name;
	
	//根据多个设备ID批量查询
	private String[] faci_ids;
	
	private String faci_ip;
	
	//分页信息
	private Page page;
	
	private Integer bureauId;

	public String getFaci_ip() {
		return faci_ip;
	}

	public void setFaci_ip(String faci_ip) {
		this.faci_ip = faci_ip;
	}

	public Integer getBureauId() {
		return bureauId;
	}

	public void setBureauId(Integer bureauId) {
		this.bureauId = bureauId;
	}

	public String[] getFaci_ids() {
		return faci_ids;
	}

	public void setFaci_ids(String[] faci_ids) {
		this.faci_ids = faci_ids;
	}

	public String getFaci_name() {
		return faci_name;
	}

	public void setFaci_name(String faci_name) {
		this.faci_name = faci_name;
	}

	public Page getPage() {
		return page;
	}

	public void setPage(Page page) {
		this.page = page;
	}

}
