package org.infosec.ismp.agent.winsensor.register;

import java.io.Serializable;

/**
 * @author Rocky
 * @version create timeï¼šOct 14, 2010 8:09:07 PM
 * 
 */
public class WinsensorRegisterInfo implements Serializable{
	private static final long serialVersionUID = -7441334202074575168L;

	/*
	 * Sensor client ipAddress. not null.
	 */
	private String ip;	
	
	/*
	 * Sensor client macAddress. not null.
	 */
	private String mac;
	
	/*
	 * Sensor client unique identifier. not null.
	 */
	private String sensorId;
	
	/*
	 * Sensor client survival time. not null. 
	 */
	private String activeTime;
	
	/*
	 * Sensor client machine name. 
	 */
	private String name;
	
	/*
	 * Sensor client machine workGroup.
	 */
	private String workGroup;
	
	/*
	 * Sensor client version. not null.
	 */
	private String winsensorVersion;
	
	/*
	 * Sensor client service version. not null.
	 */
	private String winsensorServiceVersion;

	public String getIp() {
		return ip;
	}

	public void setIp(String ip) {
		this.ip = ip;
	}

	public String getMac() {
		return mac;
	}

	public void setMac(String mac) {
		this.mac = mac;
	}

	public String getSensorId() {
		return sensorId;
	}

	public void setSensorId(String sensorId) {
		this.sensorId = sensorId;
	}
	
	public String getActiveTime() {
		return activeTime;
	}

	public void setActiveTime(String activeTime) {
		this.activeTime = activeTime;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getWorkGroup() {
		return workGroup;
	}

	public void setWorkGroup(String workGroup) {
		this.workGroup = workGroup;
	}

	public String getWinsensorVersion() {
		return winsensorVersion;
	}

	public void setWinsensorVersion(String winsensorVersion) {
		this.winsensorVersion = winsensorVersion;
	}

	public String getWinsensorServiceVersion() {
		return winsensorServiceVersion;
	}

	public void setWinsensorServiceVersion(String winsensorServiceVersion) {
		this.winsensorServiceVersion = winsensorServiceVersion;
	} 
}
